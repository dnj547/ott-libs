{
    "projectName": "ott-libs",
    "version": "2.0",
    "frontend": "javascript",
    "javascript": {
        "framework": "none",
        "config": {
            "SourceDir": "src",
            "DistributionDir": "dist",
            "BuildCommand": "cd ott-libs-backend && rails db:create && rails db:migrate",
            "StartCommand": "rails s && cd ../ott-libs-frontend && open index.html"
        }
    },
    "providers": [
        "awscloudformation"
    ]
}